    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.9.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "rimraf": "^2.7.1",
    "style-loader": "^0.23.1",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "websocket-extensions": "^0.1.4"
  },
  "dependencies": {
    "prop-types": "^15.7.2",
    "react-input-position": "^1.3.1"
  }
